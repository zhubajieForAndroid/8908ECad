<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#12242E"
                android:orientation="vertical">
    <ImageView
        android:id="@+id/toobar_bg_image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <include
        android:id="@+id/system_topbar"
        layout="@layout/topbar_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:id="@+id/system_many_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/system_topbar"
        android:layout_marginLeft="@dimen/x40"
        android:layout_marginRight="@dimen/x40"
        android:layout_marginTop="@dimen/y45"
        android:background="@mipmap/guanyu_bg_4">

        <!-- 硬件版本-->
        <ImageView
            android:id="@+id/device_hardware_version_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x368"
            android:layout_marginTop="@dimen/y140"
            android:src="@mipmap/digit_0"/>
        <ImageView
            android:id="@+id/device_hardware_version_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y140"
            android:layout_toRightOf="@+id/device_hardware_version_1"
            android:src="@mipmap/digit_0"/>

        <!-- app版本-->
        <ImageView
            android:id="@+id/device_app_version_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_hardware_version_1"
            android:layout_marginLeft="@dimen/x368"
            android:layout_marginTop="@dimen/y55"
            android:src="@mipmap/digit_0"/>
        <ImageView
            android:id="@+id/device_app_version_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_hardware_version_1"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y55"
            android:layout_toRightOf="@+id/device_app_version_1"
            android:src="@mipmap/digit_0"/>

        <!-- 主控板软件版本-->
        <ImageView
            android:id="@+id/device_software_version_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_app_version_1"
            android:layout_marginLeft="@dimen/x368"
            android:layout_marginTop="@dimen/y50"
            android:src="@mipmap/digit_0"/>
        <ImageView
            android:id="@+id/device_software_version_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_app_version_1"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y50"
            android:layout_toRightOf="@+id/device_software_version_1"
            android:src="@mipmap/digit_0"

        />
        <!-- 设备序列号-->
        <ImageView
            android:id="@+id/devices_sequence_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x305"
            android:layout_marginTop="@dimen/y52"
            android:src="@mipmap/digit_0"
        />
        <ImageView
            android:id="@+id/devices_sequence_number2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x36"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_sequence_number"
            android:src="@mipmap/digit_0"
        />
        <ImageView
            android:id="@+id/devices_sequence_number3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x36"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_sequence_number2"
            android:src="@mipmap/digit_0"
        />


        <ImageView
            android:id="@+id/devices_sequence_number4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x45"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_sequence_number3"
            android:src="@mipmap/digit_0"
        />

        <ImageView
            android:id="@+id/devices_sequence_number5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_sequence_number4"
            android:src="@mipmap/digit_0"
        />


        <ImageView
            android:id="@+id/devices_sequence_number6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x39"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_sequence_number5"
            android:src="@mipmap/digit_0"
        />

        <ImageView
            android:id="@+id/devices_sequence_number7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x38"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_sequence_number6"
            android:src="@mipmap/digit_0"
        />
        <ImageView
            android:id="@+id/devices_sequence_number8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/device_software_version_1"
            android:layout_marginLeft="@dimen/x36"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_sequence_number7"
            android:src="@mipmap/digit_0"
        />

        <!--生产日期 -->
        <ImageView
            android:id="@+id/devices_init_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x305"
            android:layout_marginTop="@dimen/y52"
            android:src="@mipmap/digit_2"
        />
        <ImageView
            android:id="@+id/devices_init_date2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x38"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_init_date"
            android:src="@mipmap/digit_0"
        />

        <ImageView
            android:id="@+id/devices_init_date3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x38"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_init_date2"
            android:src="@mipmap/digit_0"
        />
        <ImageView
            android:id="@+id/devices_init_date4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x42"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_init_date3"
            android:src="@mipmap/digit_0"
        />


        <ImageView
            android:id="@+id/devices_init_date5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x100"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_init_date4"
            android:src="@mipmap/digit_0"
        />
        <ImageView
            android:id="@+id/devices_init_date6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x38"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_init_date5"
            android:src="@mipmap/digit_0"
        />
        <ImageView
            android:id="@+id/devices_init_date7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x100"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_init_date6"
            android:src="@mipmap/digit_0"
        />
        <ImageView
            android:id="@+id/devices_init_date8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/devices_sequence_number"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y52"
            android:layout_toRightOf="@+id/devices_init_date7"
            android:src="@mipmap/digit_0"
        />
    </RelativeLayout>

    <ImageButton
        android:id="@+id/about_system_cheack_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="@dimen/y19"
        android:layout_marginLeft="@dimen/x40"
        android:background="#12242E"
        android:src="@mipmap/btn_4"/>
    <ImageButton
        android:id="@+id/about_system_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="@dimen/y19"
        android:layout_marginRight="@dimen/x40"
        android:background="#12242E"
        android:src="@mipmap/back_red"
    />

</RelativeLayout>
